    <div id="kp-wrapper">
        <style>
        #kp-wrapper * {
            box-sizing: border-box;
        }
        #kp-btn {
            position: fixed;
            width: 30px;
            height: 30px;
            font-size: 20px;
        }
        #kp-dialog {
            z-index: 100000;
            display: none;
            position: fixed;
            width: 300px;
            height: 250px;
            font-size: 20px;
            top: 0;
            bottom: 0;
            left: 0;
            right: 0;
            margin: auto;
            padding: 30px;
            border: 5px solid;
            box-sizing: border-box;
            background-color: white;
        }
        #kp-login {
            z-index: 100001;
            display: none;
            position: fixed;
            width: 300px;
            height: 250px;
            font-size: 20px;
            top: 0;
            bottom: 0;
            left: 0;
            right: 0;
            margin: auto;
            padding: 30px;
            border: 5px solid;
            box-sizing: border-box;
            background-color: white;
        }
        #kp-overlay {
            display: none;
            position: fixed;
            top: -200px;
            bottom: -200px;
            left: -200px;
            right: -200px;
            z-index: 99999;
            background-color: black;
            opacity: 0.7;
        }
        #kp-signedInStatus {
            padding-left: 30%;
            width: 100%;
            text-align: right;
            font-size: 11px;
        }
        #kp-heading {
            margin: 0;
            text-align: center;
        }
        #kp-selection {
            font-size: 16px;
            font-family: monospace;
            height: 40px;
            overflow: hidden;
        }
        #kp-tagHolder {
            min-height: 20px;
            margin-bottom: 5px;
        }
        .kp-tagBox {
            border: 1px silver solid;
            display: inline-block;
            padding: 0 14px 0 5px;
            position: relative;
            margin-right: 5px;
            font-size: 15px;
        }
        .kp-tagBox .cross {
            position: absolute;
            top: 2px;
            bottom: 0;
            padding-right: 3px;
            right: 0;
            font-size: 12px;
            cursor: pointer;
        }
        #kp-tagName {
            display: block;
            width: 100%;
            padding: 5px;
            margin-bottom: 15px;
        }
        #kp-tagName.emptyError {
            border: 2px solid red;
        }
        #kp-btnHolder {
            font-size: 0;
        }
        .kp-actionBtn {
            width: 50%;
            min-height: 30px;
        }
        #kp-loader,
        #kp-confirmation,
        #kp-regret {
            display: none;
        }
        </style>
        <!--<script src="https://apis.google.com/js/client:platform.js" async defer></script>-->
        <button id="kp-btn" style="display: none;">#</button>
        <div id="kp-dialog">
            <div id="kp-signedInStatus">Not signed in</div>
            <h2 id="kp-heading">Keeper</h2>
            <div id="kp-saveForm">
                <p id="kp-selection">{selection}</p>
                <div id="kp-tagHolder"></div>
                <input id="kp-tagName" type="text" placeholder="Your tag name goes here"></input>
                <div id="kp-btnHolder">
                    <button id="kp-saveTag" class="kp-actionBtn">Save Tag</button>
                    <button id="kp-authSaveTag" style="display: none" class="kp-actionBtn">Auth &amp; Save Tag</button>
                    <button class="kp-actionBtn kp-cancel">Cancel</button>
                </div>
            </div>
            <div id="kp-loader">
                ...a moment while we save your tag...
            </div>
            <div id="kp-success">
                <p class="kp-message">Yay! Tag saved.</p>
                <button class="kp-actionBtn kp-cancel">OK</button>
            </div>
            <div id="kp-regret">
                <p class="kp-message">Oops! Looks like something went wrong. Lets try saving that tag again?</p>
                <button id="kp-tryAgain" class="kp-actionBtn">Yeah, Sure</button>
                <button class="kp-actionBtn kp-cancel">Nah! I'l do that later</button>
            </div>
        </div>
        <div id="kp-login"></div>

        <div id="kp-overlay"></div>
    </div>
</body>
<script src="/js/getSelection.js"></script>
{/main}
